.btn {
  display: inline-block;
}
.btn.primary {
  background-color: #2cbbb6;
  color: #ffffff;
  text-align: center;
  font-size: 0.875rem;
  font-weight: 500;
  display: inline-block;
}
.btn.secondary {
  background-color: #23becc;
  color: #ffffff;
  text-align: center;
  font-size: 0.875rem;
  font-weight: 500;
  display: inline-block;
}
.btn.third {
  background-color: #282a37;
  color: #ffffff;
  text-align: center;
  font-size: 0.875rem;
  font-weight: 500;
  display: inline-block;
}
.btn.wide {
  width: 100%;
  padding: 1.1875rem 0;
  border-radius: 0.125rem;
  font-size: 1rem;
  margin-top: 1.875rem;
  border-radius: 0.25rem;
}
.btn.more {
  color: #2cbbb6;
  font-size: 0.875rem;
}
.btn.more span {
  display: inline-block;
  vertical-align: middle;
}
.btn.more .icon-more {
  margin-left: 10px;
}
.btn.line {
  display: inline-block;
  border: 0.125rem solid #2cbbb6;
  padding: 0.5625rem 0.875rem;
  color: #2cbbb6;
  font-size: 0.875rem;
  font-weight: 500;
}
.btn.line.icon {
  min-width: 10.3125rem;
}
.btn.line.icon .icon-btn-arrow {
  width: 0.5rem;
  height: 0.875rem;
  background-size: contain;
}
.btn.icon [class*=icon-] {
  margin-right: 0.625rem;
  vertical-align: middle;
  background-size: contain;
  height: 1rem;
}
.btn.icon.icon-right [class*=icon-] {
  position: absolute;
  right: 1.875rem;
  top: 50%;
  transform: translateY(-50%);
}
.btn.icon.icon-left [class*=icon-] {
  float: left;
}
.btn.sub-dark {
  background-color: #282a37;
  color: #ffffff;
}
.btn.gray {
  background-color: #999999;
  color: #ffffff;
}
.btn.disabled {
  cursor: default;
  background-color: #aaaaaa;
  color: #ffffff;
}
.btn.btn-default {
  min-width: 15rem;
  text-align: center;
  padding: 1.125rem 0;
  font-size: 1.125rem;
  font-weight: 500;
  border-radius: 0.25rem;
}

.fit-btn-wrap {
  bottom: 0;
  left: 0;
  right: 0;
  display: block;
  height: 6.875rem;
  box-sizing: content-box;
  margin-top: 6.25rem;
  height: 3.4375rem;
}
.fit-btn-wrap .btn {
  position: absolute;
  left: 0;
  margin-top: 0;
  height: 6.875rem;
  text-align: center;
  border-radius: 0;
}
.fit-btn-wrap .btn {
  width: 100%;
  height: auto;
  border-radius: 0;
}

.btn-wrap {
  position: relative;
  text-align: center;
}
.btn-wrap .right {
  position: absolute;
  right: 0;
  top: 0;
}

