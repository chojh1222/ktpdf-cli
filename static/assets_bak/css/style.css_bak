    text-align: center;
    font-size: 2rem;
    font-weight: 500;
    display: inline-block;
  }
  .btn.secondary {
    background-color: #23becc;
    color: #ffffff;
    text-align: center;
    font-size: 2rem;
    font-weight: 500;
    display: inline-block;
  }
  .btn.third {
    background-color: #282a37;
    color: #ffffff;
    text-align: center;
    font-size: 2rem;
    font-weight: 500;
    display: inline-block;
  }
  .btn.wide {
    width: 100%;
    padding: 2.375rem 0;
    border-radius: 0.25rem;
    margin-top: 1.875rem;
    font-size: 2rem;
  }
  .btn.line {
    display: inline-block;
    border: 0.125rem solid #2cbbb6;
    padding: 1.125rem 1.75rem;
    color: #2cbbb6;
    font-size: 1.75rem;
    font-weight: 500;
  }
  .btn.line.icon-right {
    min-width: 14.375rem;
  }
  .btn.line.icon-right .icon-btn-arrow {
    width: 0.875rem;
    height: 1.5rem;
    background-size: contain;
  }
  .btn.icon-right [class*=icon-] {
    float: right;
  }
  .btn.icon-left [class*=icon-] {
    float: left;
  }
  .btn.sub-dark {
    background-color: #282a37;
    color: #ffffff;
  }
  .btn.disabled {
    cursor: default;
    background-color: #aaaaaa;
    color: #ffffff;
  }

  .fit-btn-wrap {
    bottom: 0;
    left: 0;
    right: 0;
    display: block;
    height: 6.875rem;
    box-sizing: content-box;
    margin-top: 6.25rem;
  }
  .fit-btn-wrap .btn {
    position: absolute;
    left: 0;
    margin-top: 0;
    height: 6.875rem;
    text-align: center;
    border-radius: 0;
  }
}
@media screen and (max-width: 1000px) {
  .btn.wide {
    border-radius: 0.125rem;
    padding: 2.375rem 0;
  }
}
.dropdown {
  box-sizing: border-box;
}
.dropdown.complementary .toggle {
  display: inline-block;
  position: relative;
  font-size: 0.875rem;
  border: 0.0625rem solid #333333;
  border-radius: 0.25rem;
  padding: 0.875rem 1.375rem;
  min-width: 10rem;
  color: #ffffff;
  border-color: #b3b3b3;
}
.dropdown.complementary .toggle.on:after {
  content: "";
  display: inline-block;
  position: absolute;
  right: 1.375rem;
  width: 0.5625rem;
  height: 0.3125rem;
  background-image: url(../images/icon-select.png);
  background-repeat: no-repeat;
  top: 50%;
  transform: rotate(180deg) translateY(50%);
  border-color: #b3b3b3;
  transform: rotate(0deg) translateY(-50%);
}
.dropdown.complementary .toggle:after {
  content: "";
  display: inline-block;
  position: absolute;
  right: 1.375rem;
  width: 0.5625rem;
  height: 0.3125rem;
  background-image: url(../images/icon-select.png);
  background-repeat: no-repeat;
  top: 50%;
  transform: rotate(180deg) translateY(50%);
  border-color: #b3b3b3;
}
.dropdown.complementary .dropdown-menu {
  display: none;
  position: absolute;
  min-width: 10rem;
  overflow: hidden;
  background-color: #ffffff;
  top: 2.75rem;
}
.dropdown.complementary .dropdown-menu li {
  padding: 0.1875rem;
}
.dropdown.complementary .dropdown-menu li:hover {
  background-color: lightgray;
}
.dropdown.complementary .dropdown-menu li a {
  display: block;
  font-size: 0.875rem;
}
.marker {
  position: relative;
  font-weight: 500;
  z-index: 1;
}
.marker:after {
  content: "";
  position: absolute;
  bottom: -0.1875rem;
  left: 0;
  height: 1.25rem;
  width: 100%;
  background-color: #9cd5d3;
  z-index: -1;
}

.accordion li {
  border-bottom: 0.0625rem solid #d5d5d5;
  cursor: pointer;
}
.accordion li.toggle a {
  font-size: 1.25rem;
  font-weight: 500;
}
.accordion li.toggle:after {
  position: absolute;
  right: 1.625rem;
  content: "";
  display: inline-block;
  width: 1.5rem;
  height: 0.875rem;
  background-image: url(../images/icon-accor-arrow.png);
  background-repeat: no-repeat;
  transform: rotate(180deg);
  transition: all 0.2s;
  background-size: contain;
  top: 50%;
}
.accordion li.toggle.on:after {
  transform: rotate(0deg) translateY(-50%);
}
.accordion li.contents {
  height: 0;
  padding: 0 3.125rem;
  overflow: hidden;
  background-color: #f9f9f9;
  font-size: 1rem;
  color: #666666;
  line-height: 1.75;
  transition: all 0.2s;
}
.accordion li.contents.on {
  height: auto;
  padding: 1.875rem 3.125rem;
}

